<% form_for @taxon_concept do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :comments %><br />
    <%= f.text_area :comments %>
  </p>
  <p>
    <%= f.label :taxon_name %><br />
    <%= f.text_field :taxon_name %>
  </p>
  <p>
    <%= f.label :has_taxon_status %><br />
    <%= f.text_field :has_taxon_status %>
  </p>
  <p>
    <%= f.label :rank %><br />
    <%= f.text_field :rank %>
  </p>
  <p>
    <%= f.label :is_current %><br />
    <%= f.check_box :is_current %>
  </p>
  <p><%= f.submit %></p>
<% end %>
