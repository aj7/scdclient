<% semantic_form_for common_name do |f| %>
    <%#= f.error_messages %>
    <% f.inputs do	%>
        <%= f.input :language, :prompt => "Select language" %>
        <%= f.input :name %>
        <%= f.input :preferred %>

    <% end -%>
    <% f.buttons do %>
        <%= f.commit_button %>
    <% end -%>

<% end -%>
